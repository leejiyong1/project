<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="m">
	<select id="idcheck" resultType="member" parameterType="String">
		select * from member where id = #{id}
	</select>
	
	<insert id="join_insert" parameterType="member">
		insert into member values(seq_mem_idx.nextval,#{id},#{pwd},#{name},#{addr},sysdate,010-1111-1111,mega@naver.com,990706,male)
	</insert>
</mapper>












